	<queryString>
		<![CDATA[SELECT extract(day from a.fecha_pedido) as Dia, Sum(p.cantidad * i.precio) as Cantidad_Total 
FROM item_pedido p, item i , pedido a
where p.id_item = i.id_item AND p.id_pedido = a.id_pedido AND a.fecha_pedido between $P{fecha_inicio} AND $P{fecha_fin}
group by extract(day from a.fecha_pedido)
order by extract(day from a.fecha_pedido) Asc Limit 10]]>
	</queryString>
