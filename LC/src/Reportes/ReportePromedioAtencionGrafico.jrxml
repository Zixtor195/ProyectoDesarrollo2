	<queryString>
		<![CDATA[SELECT p.id_empleado, e.nombres, extract(hour from AVG(p.hora_ultimo_item - p.hora_inicio)) as Promedio 
FROM item_pedido i, pedido p, empleado e, item a
where 
p.id_empleado = e.id_empleado AND i.id_item = a.id_item AND p.id_pedido = i.id_pedido
AND p.fecha_pedido Between '01/02/2016' AND '30/06/2016'
group by p.id_empleado, e.nombres
order by AVG(p.hora_ultimo_item - p.hora_inicio) Asc]]>
	</queryString>
	<field name="id_empleado" class="java.lang.Integer"/>
	<field name="nombres" class="java.lang.String"/>
	<field name="promedio" class="java.lang.Double"/>
